package in.sp.bean;

public class User {
	String name;
	String email;
	String password;
	int roll_no;
	
	public void display() {
		System.out.println("Name : "+name);
		System.out.println("Email : "+email);
		System.out.println("Password : "+password);
		System.out.println("Roll_no : "+roll_no);
	}
}
